<%= form_for @conn, session_path(@conn, :create), fn f -> %>
  <label>
    Email: <%= text_input f, :email, class: "qa-session-email" %>
  </label>

  <label>
    Password: <%= password_input f, :password, class: "qa-session-password" %>
  </label>

  <%= submit "Submit", class: "qa-session-submit" %>
<% end %>
